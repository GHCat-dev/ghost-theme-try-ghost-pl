{
    "name": "gatsby-transform-html",
    "description": "Gatsby plugin to transform html into hast to be used by child plugins",
    "version": "1.0.0",
    "author": "styxlab",
    "dependencies": {
        "@babel/runtime": "7.7.2",
        "bluebird": "3.7.1",
        "sharp": "0.22.1",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@babel/cli": "7.7.0",
        "@babel/core": "7.7.2",
        "babel-preset-gatsby-package": "0.2.11",
        "cross-env": "6.0.3"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "favicon",
        "icons",
        "manifest.webmanifest",
        "progressive-web-app",
        "pwa"
    ],
    "resolutions": {
        "sharp": "0.22.1"
    },
    "license": "MIT",
    "main": "index.js",
    "peerDependencies": {
        "gatsby": ">2.0.0-alpha"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore **/__tests__"
    }
}
