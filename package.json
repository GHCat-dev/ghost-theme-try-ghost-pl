{
  "name": "gatsby-theme-try-ghost-workspace",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "develop": "yarn workspace try-ghost develop",
    "build": "yarn workspace try-ghost build",
    "clean": "yarn workspace try-ghost clean",
    "serve": "yarn workspace try-ghost serve",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "pack": "lerna run build --scope={$(ls ./packages | xargs echo | sed 's/ /,/g')}"
  },
  "workspaces": [
    "packages/*",
    "gatsby-theme-try-ghost",
    "try-ghost"
  ],
  "devDependencies": {
    "lerna": "^3.20.2"
  }
}
