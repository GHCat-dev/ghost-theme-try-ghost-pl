{
    "name": "gatsby-transformer-rehype",
    "description": "Gatsby plugin to transform html into hast to be used by child plugins",
    "version": "1.0.0",
    "author": "styxlab",
    "main": "gatsby-node.js",
    "dependencies": {
        "@babel/runtime": "7.7.2",
        "bluebird": "3.7.1",
        "hast-util-raw": "^5.0.2",
        "lodash": "^4.17.15",
        "rehype": "^9.0.1",
        "unist-util-remove-position": "^2.0.1"
    },
    "devDependencies": {
        "@babel/cli": "7.7.0",
        "@babel/core": "7.7.2",
        "babel-preset-gatsby-package": "0.2.11",
        "cross-env": "6.0.3"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "html",
        "rehype"
    ],
    "license": "MIT",
    "peerDependencies": {
        "gatsby": ">2.0.0-alpha"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore **/__tests__",
        "develop": "babel src --out-dir . --ignore **/__tests__"
    }
}
